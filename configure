${GROFF_NOTE}
${GROFF_NOTE}.1.ps :
${GROFF_NOTE}	\$(SHELL) -c "tbl \$*.1 | groff -man" >\$@
${GROFF_NOTE}
${GROFF_NOTE}.1.txt :
${GROFF_NOTE}	GROFF_NO_SGR=stupid \$(SHELL) -c "tbl \$*.1 | nroff -rHY=0 -Tascii -man | col -bx" >\$@

${MAN2HTML_NOTE}.1.html :
${MAN2HTML_NOTE}	./${MAN2HTML_TEMP} \$* 1 man >\$@

CF_EOF
	cf_make_docs=1
fi

